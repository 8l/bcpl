GET "libhdr"

LET start(n) = VALOF
{ LET a, b, c = 0, 1, ?
  LET i = 0

  writef("position %n  value %n*n", i, a}
  c := a+b
  a := b
  b := c
  i := i+1

  writef("position %n  value %n*n", i, a}
  c := a+b
  a := b
  b := c
  i := i+1

  writef("position %n  value %n*n", i, a}
  c := a+b
  a := b
  b := c
  i := i+1


  RESULTIS 0
}
